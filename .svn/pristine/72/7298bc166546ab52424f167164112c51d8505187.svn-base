header{
    background-color: #f0f0f0;
    margin: 0 auto;
    width: 100%;
    height: 195px;
    display: block;
}
.main{
    width: 100%;
}
.main .search{
    margin:0 auto;
    width: 100%;
    max-width: 800px;
}
.main .search .search-type{

}
.main .search .search-type .type-baidu,.main .search .search-type .type-google{
    background-color: #D0D0D0;
    width: 60px;
    color: #6F6F6F;
    text-align: center;
    line-height: 40px;
    float: left;
}
.main .search .search-type .type-google{
    background-color: #e6e6e6;
}
.main .search .search-type .selected{
    background-color: #676767;
    color: #ffffff;
}
.main .search .search-input{
    box-sizing: border-box;
    padding: 2px 2px 2px 6px;
    max-width: 800px;
    width: 80%;
    height: 40px;
    font-size: 17px;
    border-color: #e6e6e6;
    border-top: none;
    outline: none;
    float: left;
}
.main .search .search-btn{
    background-color: #e6e6e6;
    width: 20%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    cursor: pointer;
    float: right;
}
.main .search .search-btn:hover{
    background-color: #D0D0D0;
    color: #ffffff;
}
.main .web-list{
    margin:0 auto;
    width: 100%;
    max-width: 800px;
}
.main .web-list .list-type-header{
    margin-top: 15px;
    background-color: #BCCEFC;
    height: 40px;
    line-height: 40px;
    text-indent: 12px;
    color: #565656;
}
.main .list-contents {
    background-color: #F9F9F9;
    padding-left: 5px;
}
.main .list-contents .list-contents-one{
    background-color: #D0EFFF;
    margin: 10px 10px 10px 0;
    padding: 0 5px;
    height: 30px;
    line-height: 30px;
    float: left;
}
.main .list-contents .list-contents-one a{
    color: #BBA404;
}